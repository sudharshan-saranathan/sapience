* {
	border					: none;
	color                   : white;
	background				: white;
}

QFrame {
    border                  : none;
    background              : white;
}

QComboBox {
    border: 1px solid #d0d0d0; /* Light gray border for macOS look */
    border-radius: 6px; /* Rounded corners */
    background: #f8f8f8; /* Light background for macOS look */
    color: #333; /* Text color */
}

QComboBox:hover {
    border: 1px solid #a0a0a0; /* Slightly darker border on hover */
}

QComboBox::drop-down {
    border: none; /* No additional border for drop-down */
    width: 20px;
    background: transparent;
}

QComboBox::down-arrow {
    image: url(down_arrow.png); /* Use a custom down-arrow icon */
    width: 8px;
    height: 8px;
}

QComboBox QAbstractItemView {
    border: 1px solid #d0d0d0; /* Border for the drop-down list */
    background: #ffffff; /* White background for items */
    selection-background-color: #0078d7; /* macOS-style blue selection */
    selection-color: #ffffff; /* White text on selection */
}

QTextEdit {
    border                  : 1px solid black;
    font                    : 14px "Menlo";
    color                   : white;
    background              : #696D7D;
}

QMenu {
    font-family             : "Gill Sans";
    border                  : 1px solid black;
    padding                 : 4px 4px 4px 4px;
    color                   : black;
    background              : white;
    border-radius           : 8px;
}

QMenu::item {
    padding                 : 4px 4px 4px 4px;
    color                   : black;
    background-color        : white;
}

QMenu::item:disabled {
    padding                 : 4px 4px 4px 4px;
    color                   : gray;
    background-color        : white;
}

QMenu::item:selected {
    padding                 : 4px 4px 4px 4px;
    color                   : #187795;
    background              : #dddddd;
    border-radius           : 6px;
}

QLabel {
    color                   : black;
    border                  : 1px solid black;
    padding                 : 1px;
    background              : white;
    border-radius           : 10px;
}

QToolTip {
    border                  : 1px solid black;
    color                   : black;
    font-family             : "Gill Sans";
    font-size               : 14px;
    background              : #D0F4EA;
}

QRadioButton {
    color                   : black;
    background              : transparent;
}

QSplitter {
	border                  : none;
	background              : white;
}

QSplitter::handle:horizontal {

	width                   : 2px;
	border                  : 2px solid black;
	border-radius           : 2px;
	background				: black;
}

QSplitter::handle:vertical {

	width                   : 2px;
	border                  : 2px solid black;
	border-radius           : 2px;
	background				: black;
}

QMenuBar {
	color              		: white;
	border					: 2px solid white;
	border-radius           : 2px;
	padding                 : 2px;
	background				: #344c64;
}

QStatusBar {
	border					: none;
	padding                 : 2px;
	background				: #22333B;
}

QGroupBox {
    padding                 : 0px;
    border-radius           : 16px;
    background              : #64a6bd;
}

QTabWidget::pane {
    background              : white;
}

QTabWidget::tab-bar:left {
    right                   : 2px;
}

QTabBar {
    border                  : 1px solid black;
    padding                 : 1px 1px 1px 1px;
    color                   : black;
    background              : #64a6bd;
}

QTabBar::tab {
    width                   : 18px;
    height                  : 301px;
    padding                 : 1px 3px 1px 1px;
    background              : transparent;
}

QTabBar::tab:selected {
    background              : #c6dee7;
    background-clip         : content;
}

QTabBar::tab:!selected:hover {
    background              : #07D6DA;
    background-clip         : content;
}

