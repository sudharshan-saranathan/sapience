QToolButton {
	border					: none;
	padding                 : 2px 2px;
	background				: #64a6bd;
	border-radius			: 12px;
}

QToolButton:hover {
	border					: none;
	padding                 : 2px 2px;
	background				: white;
	border-radius			: 12px;
}

QToolButton:pressed {
	border					: none;
	padding                 : 2px 2px;
	background				: #c6dee7;
	border-radius			: 12px;
}

QToolButton:checked {
	border					: none;
	padding                 : 2px 2px;
	background				: #c6dee7;
	border-radius			: 12px;
}

QPushButton {
	border					: 2px solid black;
	padding					: 0px 0px;
	background				: #c6dee7;
	border-radius           : 14px;
	background-clip         : content;
}

QPushButton:hover {
    border                  : 2px solid #c6dee7;
    background              : white;
}

QPushButton:pressed {
	border                  : 2px solid white;
	background				: white;
}

QCheckBox {
	border					: none;
	background-color		: transparent;
}

QCheckBox::indicator:hover {
	border					: none;
	background-color		: grey;
}

QCheckBox::indicator:checked {
	background-color		: #EB9500;
}

QLineEdit {
	border					: 1px solid black;
	border-radius           : 6px;
	padding                 : 2px 2px 2px 2px;
	color                   : white;
	background-color      	: #4C5C68;
	placeholder-text-color  : gray;
}

QScrollBar:horizontal {
    height					: 12px;
	border                  : none;
	margin                  : 1px 1px 1px 1px;
    background				: transparent;
}

QScrollBar::handle:horizontal {
	border					: 1px solid gray;
	height                  : 6px;
	min-width           	: 50px;
    background				: gray;
	border-radius			: 4px;
}

QScrollBar::handle:hover:horizontal {
	border					: 1px solid #aaaaaa;
	min-width               : 50px;
    background				: #aaaaaa;
}

QScrollBar::add-line:horizontal, QScrollBar::sub-line:horizontal {
	border					: none;
	background-image        : transparent;
}

QScrollBar:vertical {
    width					: 12px;
	border            		: none;
	margin                  : 1px 1px 1px 1px;
    background				: transparent;
}

QScrollBar::handle:vertical {
	border					: 1px solid gray;
	width                   : 6px;
	min-height              : 50px;
    background				: gray;
	border-radius			: 4px;
}

QScrollBar::handle:hover:vertical {
	border					: 2px solid #aaaaaa;
	min-width               : 50px;
    background				: #aaaaaa;
}

QScrollBar::add-line:vertical, QScrollBar::sub-line:vertical{
	border					: none;
	background 				: transparent;
}

QTableWidget {
    border: 1px solid black;
    color: black;
    font: 12px "Verdana";
}

QHeaderView::section {
    color: black;
    font: 12px "Gill Sans";
}

QListView {
     font-family             : "Gill Sans";
     border                  : 1px solid black;
     padding                 : 4px 4px 4px 4px;
     color                   : black;
     background              : white;
     border-radius           : 8px;
 }

 QListView::item {
     padding                 : 4px 4px 4px 4px;
     color                   : black;
     background-color        : white;
 }

 QListView::item:disabled {
     padding                 : 4px 4px 4px 4px;
     color                   : gray;
     background-color        : white;
 }

 QListView::item:selected {
     padding                 : 4px 4px 4px 4px;
     color                   : #187795;
     background              : #dddddd;
     border-radius           : 6px;
 }